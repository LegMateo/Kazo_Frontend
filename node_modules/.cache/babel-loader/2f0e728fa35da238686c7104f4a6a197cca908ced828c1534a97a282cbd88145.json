iv>\n    <h2>Sign up</h2>\n    <div class=\"form-group\">\n      <input type=\"text\" v-model=\"username\" placeholder=\"Username\" :class=\"{ 'invalid': !isUsernameValid }\">\n      <div v-if=\"!isUsernameValid\" class=\"error-msg\">Must be at least 6 digits.</div>\n    </div>\n    <div class=\"form-group\">\n      <input type=\"password\" v-model=\"password\" placeholder=\"Password\" :class=\"{ 'invalid': !isPasswordValid }\">\n      <div v-if=\"!isPasswordValid\" class=\"error-msg\">Must be at least 6 digits.</div>\n    </div>\n    <div class=\"form-group\">\n      <label>Arrival date:</label>\n      <input type=\"date\" v-model=\"arrivalDate\" :class=\"{ 'invalid': !isArrivalDateValid }\">\n      <div v-if=\"!isArrivalDateValid\" class=\"error-msg\">Please enter arrival date.</div>\n    </div>\n    <div class=\"form-group\">\n      <label>Departure date:</label>\n      <input type=\"date\" v-model=\"departureDate\" :class=\"{ 'invalid': !isDepartureDateValid }\">\n      <div v-if=\"!isDepartureDateValid\" class=\"error-msg\">Please enter departure date.</div>\n    </div>\n\n    <button class=\"signup-btn\" @click=\"signup\">Sign up</button>\n  </div>\n</template>\n\n<style scoped>\n.form-group {\n  margin-bottom: 20px;\n}\ninput, select {\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n}\nlabel {\n  display: block; \n  font-weight: bold;\n}\n.signup-btn {\n  padding: 10px;\n  background-color: #007bff;\n  color: #fff;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n.signup-btn:hover {\n  background-color: #0056b3;\n}\n.invalid {\n  border-color: red;\n}\n.error-msg {\n  color: red;\n  margin-top: 5px;\n}\n</style>\n\n<script>\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      arrivalDate: '',\n      departureDate: ''\n    };\n  },\n  computed: {\n    isUsernameValid() {\n      return this.username.length > 6;\n    },\n    isPasswordValid() {\n      return this.password.length > 6;\n    },\n    isArrivalDateValid() {\n      return !!this.arrivalDate;\n    },\n    isDepartureDateValid() {\n      return !!this.departureDate;\n    }\n  },\n  methods: {\n    async signup() {\n      if (!this.isUsernameValid || !this.isPasswordValid || !this.isArrivalDateValid || !this.isDepartureDateValid) {\n        // Provjeri valjanost korisničkog imena, lozinke i datuma\n        return;\n      }\n\n      // Ovdje dodajte kod za obradu registracije\n      // Možete koristiti podatke this.username, this.password, this.arrivalDate, this.departureDate\n      // Na primjer, možete poslati zahtjev na server za stvaranje novog korisnika\n\n      try {\n        const response = await this.$axios.post('http://localhost:3000/register', {\n          username: this.username,\n          password: this.password,\n          arrivalDate: this.arrivalDate,\n          departureDate: this.departureDate,\n        });\n\n        // Ako je registracija uspješna, možete nešto učiniti, npr. preusmjeriti korisnika na login stranicu\n        this.$router.push('/login');\n      } catch (error) {\n        // Ako je došlo do pogreške pri registraciji, obradite je ovdje\n        console.error('Greška prilikom registracije:', error);\n      }\n    },\n  },\n};\n</script>\n"],"mappings":";AA4DA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACL,QAAQ,CAACM,MAAK,GAAI,CAAC;IACjC,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACN,QAAQ,CAACK,MAAK,GAAI,CAAC;IACjC,CAAC;IACDE,kBAAkBA,CAAA,EAAG;MACnB,OAAO,CAAC,CAAC,IAAI,CAACN,WAAW;IAC3B,CAAC;IACDO,oBAAoBA,CAAA,EAAG;MACrB,OAAO,CAAC,CAAC,IAAI,CAACN,aAAa;IAC7B;EACF,CAAC;EACDO,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACN,eAAc,IAAK,CAAC,IAAI,CAACE,eAAc,IAAK,CAAC,IAAI,CAACC,kBAAiB,IAAK,CAAC,IAAI,CAACC,oBAAoB,EAAE;QAC5G;QACA;MACF;;MAEA;MACA;MACA;;MAEA,IAAI;QACF,MAAMG,QAAO,GAAI,MAAM,IAAI,CAACC,MAAM,CAA